/* pages/user/user.wxss */
page{background-color: #F5F5F5}
.userinfo{width: 100%;height: 340rpx;position: fixed;top: 0;}
.user_img{margin-left: 3%;width: 150rpx;height: 150rpx;margin-top:95rpx;border-radius: 50%;border: 1rpx solid black;}
.user_name{width: 55%;height: 200rpx;margin-top: 70rpx;margin-left:25rpx;}
.user_name_{width: 100%;font-size: 32rpx;font-weight: bolder;color: white;margin-top: 40rpx;}
.fly_user{width: 200rpx;text-align: center;height: 60rpx;line-height: 60rpx;border-radius: 5rpx;border: 1rpx solid #FEAF86;font-size: 28rpx;color: white;margin-top:20rpx;}
.user_qd{width: 100rpx;text-align: center;height: 60rpx;line-height: 60rpx;font-size: 28rpx;background-color: white;border-radius: 30rpx;color: #FF6634;margin-top:50rpx;}

.div{width: 100%;background-color: white;padding: 20rpx 0;}
.div_list{width: 80%;margin-left: 10%;height: 100rpx;border-bottom: 1rpx solid #F6F6F6;font-size: 30rpx;}
.div_list_{width: 50%;height: 60rpx;line-height: 60rpx;border-right: 1rpx solid #F4F4F4;margin-top:20rpx;}
.div_list_text{margin-left: 20rpx;}