<!--pages/user/user.wxml-->
<image src='../../img/82d222c76d8b1e3900eae70d12d3a68a.png' style='width:100%' mode='widthFix'/>
<view class='userinfo flex'>
    <view class='user_img'></view>
    <view class='user_name'>
        <view class='user_name_'>舞友：JICK</view>
        <view class='fly_user'>修改个人资料</view>
    </view>
    <view class='user_qd'>签到</view>
</view>

<view class='div'>
    <view class='div_list flex'>
        <view class='div_list_ flex' bindtap='my_collection'>
            <image src='../../img/cz.png' style='width:50rpx;margin-top:5rpx;' mode='widthFix'/>
            <view class='div_list_text'>我的收藏</view>
        </view>
         <view class='div_list_ flex'>
            <image src='../../img/cz (3).png' style='width:50rpx;margin-top:10rpx;margin-left:110rpx;' mode='widthFix'/>
            <view class='div_list_text'>我的关注</view>
        </view>
    </view>
     <view class='div_list flex'>
        <view class='div_list_ flex' bindtap='balance'>
            <image src='../../img/cz (4).png' style='width:50rpx;margin-top:5rpx;' mode='widthFix'/>
            <view class='div_list_text'>我的余额</view>
        </view>
         <view class='div_list_ flex' bindtap='my_order'>
            <image src='../../img/cz (2).png' style='width:40rpx;margin-top:10rpx;margin-left:110rpx;' mode='widthFix'/>
            <view class='div_list_text'>我的订单  </view>
        </view>
    </view>
</view>

<view class='div' style='margin-top:20rpx;'>
    <view class='div_list flex'>
        <view class='div_list_ flex' bindtap='integral'>
            <image src='../../img/cz (5).png' style='width:40rpx;margin-top:10rpx;' mode='widthFix'/>
            <view class='div_list_text'>我的积分</view>
        </view>
         <view class='div_list_ flex'>
            <image src='../../img/cz (7).png' style='width:50rpx;margin-top:10rpx;margin-left:110rpx;' mode='widthFix'/>
            <view class='div_list_text'>预约课程</view>
        </view>
    </view>
</view>

<view class='div' style='margin-top:20rpx;'>
    <view class='div_list flex'>
        <view class='div_list_ flex'>
            <image src='../../img/cz (8).png' style='width:40rpx;margin-top:10rpx;' mode='widthFix'/>
            <view class='div_list_text'>我的招聘</view>
        </view>
         <view class='div_list_ flex'>
            <image src='../../img/cz (6).png' style='width:50rpx;margin-top:10rpx;margin-left:110rpx;' mode='widthFix'/>
            <view class='div_list_text'>通用设置</view>
        </view>
    </view>
</view>







<view class='buttom flex'>
    <view class='buttom_list' bindtap='home_page'>
        <image src='../../img/fw-(5).png' class='buttom_list_imgs' mode='widthFix'/>
        <view class='buttom_list_text' >首页</view>
    </view>
    <view class='buttom_list'>
        <image src='../../img/fw-(1).png' class='buttom_list_imgs' style='width:35%;margin-left:35%' mode='widthFix'/>
        <view class='buttom_list_text'>服务</view>
    </view>
     <view class='buttom_list'>
        <image src='../../img/yjjx-11-kb.png' class='buttom_list_imgs' style='margin-top:-20rpx;width:70%;margin-left:15%' mode='widthFix'/>
        <!-- <view class='buttom_list_text'>服务</view> -->
    </view>
     <view class='buttom_list' bindtap='card'>
        <image src='../../img/fw-(3).png' class='buttom_list_imgs' style='width:32%;margin-left:37%' mode='widthFix'/>
        <view class='buttom_list_text'>购物车</view>
    </view>
     <view class='buttom_list' >
        <image src='../../img/fw-(7).png' class='buttom_list_imgs' style='width:30%;margin-left:37%'  mode='widthFix'/>
        <view class='buttom_list_text' style='color:#FF8C1F'>我的</view>
    </view>

</view>
