        <view class='div_xingx flex'>
            <view class='xix_ flex'>
                <image src='../../img/xx-12_12px.png' style='width:40rpx;margin-top:15rpx;margin-left:5rpx;' mode='widthFix' wx:for='{{5}}'/>
            </view>
            <view class='xix_money'>￥60/人</view>
        </view>

        <view class='add'>美食 红星路</view>
    </view>
    
