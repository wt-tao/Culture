        <image src='../../img/xz-1.png' style='width:20rpx;margin-left:10rpx;margin-top:35rpx;' mode='widthFix'/>
    </view>
    <view class='too_list flex'>
        <view class='top_text' style='width:60%'>美食</view>
        <image src='../../img/xz-1.png' style='width:20rpx;margin-left:10rpx;margin-top:35rpx;' mode='widthFix'/>
    </view>
    <view class='too_list flex'>
        <view class='top_text'>智能排序</view>
        <image src='../../img/xz-1.png' style='width:20rpx;margin-left:10rpx;margin-top:35rpx;' mode='widthFix'/>
